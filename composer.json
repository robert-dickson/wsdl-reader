{
    "name": "php-soap/wsdl-reader",
    "description": "A WSDL reader in PHP",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Soap\\WsdlReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soap\\WsdlReader\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "goetas-webservices/xsd-reader": "^0.4.1",
        "php-soap/engine": "^2.0",
        "php-soap/wsdl": "^1.3",
        "veewee/xml": "^2.6",
        "azjezz/psl": "^2.4",
        "symfony/console": "^6.2"
    },
    "require-dev": {
        "veewee/composer-run-parallel": "^1.2",
        "symfony/var-dumper": "^6.1",
        "php-soap/engine-integration-tests": "^1.4.2",
        "psalm/plugin-symfony": "^5.0",
        "php-standard-library/psalm-plugin": "^2.2",
        "vimeo/psalm": "^5.6"
    },
    "bin": [
        "bin/wsdl-reader"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Toon Verwerft",
            "email": "toonverwerft@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "veewee/composer-run-parallel": true
        }
    }
}
