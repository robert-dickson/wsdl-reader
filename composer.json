{
    "name": "php-soap/wsdl-reader",
    "description": "A WSDL reader in PHP",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Soap\\WsdlReader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soap\\WsdlReader\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "goetas-webservices/xsd-reader": "^0.3.10",
        "php-soap/engine": "^1.3",
        "php-soap/wsdl": "^1.3",
        "veewee/xml": "^2.2"
    },
    "require-dev": {
        "veewee/composer-run-parallel": "^1.2",
        "symfony/var-dumper": "^6.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Toon Verwerft",
            "email": "toonverwerft@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "veewee/composer-run-parallel": true
        }
    }
}
